{
  "name": "8bit-echo/sage-vite",
  "version": "9.0.9",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress starter theme with Vite workflow",
  "homepage": "https://roots.io/sage/",
  "keywords": ["wordpress", "sage", "vite"],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": "^7.4|^8.0",
    "roots/acorn": "^2.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8.0"
  },
  "scripts": {
    "test": ["phpcs"]
  }
}
